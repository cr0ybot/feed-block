{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "feed-loop/feed-loop",
	"version": "0.1.0",
	"title": "Feed Loop",
	"category": "widgets",
	"icon": "rss",
	"description": "Advanced RSS & Atom feed block with configurable child blocks.",
	"keywords": [ "rss", "atom", "feed", "embed", "loop" ],
	"attributes": {
		"feedURL": {
			"type": "string",
			"default": ""
		},
		"itemsToShow": {
			"type": "number",
			"default": 6
		},
		"displayLayout": {
			"type": "object",
			"default": {
				"type": "list"
			}
		},
		"tagName": {
			"type": "string",
			"default": "div"
		}
	},
	"providesContext": {
		"feedURL": "feedURL",
		"itemsToShow": "itemsToShow",
		"displayLayout": "displayLayout"
	},
	"supports": {
		"align": [ "wide", "full" ],
		"html": false,
		"__experimentalLayout": true
	},
	"textdomain": "feed-loop",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css"
}
