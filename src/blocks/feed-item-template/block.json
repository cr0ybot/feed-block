{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "feed-block/feed-item-template",
	"version": "0.2.0",
	"title": "Feed Item Template",
	"category": "widgets",
	"description": "Contains the block elements used to render a feed item, like the title, description, link, and more, similar to the Post Template block.",
	"parent": [ "feed-block/feed" ],
	"usesContext": [
		"feed-block/feedURL",
		"feed-block/itemsToShow",
		"feed-block/displayLayout",
		"feed-block/itemLinkRel",
		"feed-block/itemLinkTarget"
	],
	"supports": {
		"html": false
	},
	"textdomain": "feed-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css",
	"render": "file:./render.php"
}
