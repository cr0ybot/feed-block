{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "rss-block/rss-feed",
	"version": "0.1.0",
	"title": "RSS Feed",
	"category": "widgets",
	"icon": "rss",
	"description": "Advanced RSS feed with configurable child blocks.",
	"keywords": [ "rss", "feed", "embed" ],
	"attributes": {
		"feedURL": {
			"type": "string",
			"default": ""
		},
		"itemsToShow": {
			"type": "number",
			"default": 6
		},
		"displayLayout": {
			"type": "object",
			"default": {
				"type": "list"
			}
		},
		"tagName": {
			"type": "string",
			"default": "div"
		}
	},
	"providesContext": {
		"feedURL": "feedURL",
		"itemsToShow": "itemsToShow",
		"displayLayout": "displayLayout",
		"tagName": "tagName"
	},
	"supports": {
		"html": false
	},
	"textdomain": "rss-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css"
}
